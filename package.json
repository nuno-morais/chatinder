{
  "name": "chatinder",
  "productName": "Chatinder",
  "version": "1.4.3",
  "description": "Unofficial desktop messaging client for Tinder.",
  "main": "src/server.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wasd171/chatinder.git"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "publish": "cross-env NODE_ENV=production electron-forge publish",
    "precommit": "lint-staged",
    "circle:clean": "rimraf node_modules",
    "circle:artifacts": "cp ./out/make/* $CIRCLE_ARTIFACTS",
    "ci:skip": "echo 'CI detected, script skipped'",
    "install:types": "flow-typed install",
    "postinstall": "is-ci 'ci:skip' 'install:types'",
    "postmerge": "npm install",
    "test": "echo 'Someday there would be tests, I promise'"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "bugs": "https://github.com/wasd171/chatinder/issues",
  "homepage": "https://github.com/wasd171/chatinder",
  "keywords": [],
  "author": "wasd171",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg",
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "asar": {
          "unpackDir": "databases"
        },
        "icon": "./icons/icon",
        "ignore": [
          "/databases/(.*)\\.db$",
          "/flow-typed/(.*)$"
        ],
        "appBundleId": "com.wasd171.chatinder",
        "appCategoryType": "public.app-category.social-networking",
        "osxSign": {
          "identity": "Developer ID Application: Konstantin Nesterov (WB9A3TN46Q)"
        },
        "win32metadata": {
          "ProductName": "Chatinder"
        }
      },
      "electronWinstallerConfig": {
        "iconUrl": "https://wasd171.github.io/chatinder/assets/icons/icon.ico",
        "name": "chatinder",
        "setupIcon": "./icons/icon.ico",
        "remoteReleases": "https://chatinder.herokuapp.com/update/win32/1.4.3"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "wasd171",
        "name": "chatinder"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "chatinder"
      }
    }
  },
  "dependencies": {
    "about-window": "^1.6.1",
    "apollo-client": "^1.2.2",
    "bluebird": "^3.5.0",
    "date-fns": "^1.28.4",
    "electron-compile": "^6.4.1",
    "electron-squirrel-startup": "^1.0.0",
    "emojione": "2.2.7",
    "emojionearea": "^3.1.5",
    "font-awesome": "^4.7.0",
    "graphql": "^0.9.6",
    "graphql-tag": "^2.0.0",
    "graphql-tools": "^0.11.0",
    "he": "^1.1.1",
    "immutability-helper": "^2.2.0",
    "is-reachable": "^2.3.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.0",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "mobx-utils": "^2.0.1",
    "nedb": "^1.8.0",
    "node-fetch": "^1.6.3",
    "react": "^15.5.4",
    "react-addons-shallow-compare": "^15.5.2",
    "react-apollo": "^1.2.0",
    "react-dom": "^15.5.4",
    "react-image-gallery": "^0.7.16",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "react-virtualized": "^9.7.5",
    "react-waypoint": "^6.0.0",
    "redux": "^3.6.0",
    "roboto-fontface": "^0.7.0",
    "simplebar": "^2.4.1",
    "styled-components": "^1.4.6",
    "tinder-modern": "^1.2.0",
    "url-parse": "^1.1.8",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-module-resolver": "3.0.0-beta.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^4.0.0",
    "devtron": "^1.4.0",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-forge": "^3.0.0",
    "electron-prebuilt-compile": "1.6.8",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.1.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.45.0",
    "flow-typed": "^2.1.2",
    "husky": "^0.13.3",
    "is-ci-cli": "^1.1.0",
    "lint-staged": "^3.4.1",
    "prettier": "^1.3.1",
    "react-addons-perf": "^15.4.2",
    "rimraf": "^2.6.1"
  }
}

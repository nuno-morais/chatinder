{
	"name": "chatinder",
	"productName": "Chatinder",
	"version": "1.5.0",
	"description": "Unofficial desktop messaging client for Tinder.",
	"main": "dist/main.js",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/wasd171/chatinder.git"
	},
	"scripts": {
		"build:main": "webpack --config=webpack/main.js",
		"build:renderer": "webpack --config=webpack/renderer.js",
		"build:main:dev": "cross-env NODE_ENV=development npm run build:main",
		"build:renderer:dev": "cross-env NODE_ENV=development npm run build:renderer",
		"build:prod": "rimraf dist && cross-env NODE_ENV=production npm run build:main && npm run build:renderer",
		"build:dev": "concurrently \"npm run build:main:dev\" \"npm run build:renderer:dev\"",
		"publish": "npm run build:prod && electron-builder",
		"precommit": "lint-staged",
		"circle:clean": "rm -rf node_modules",
		"circle:artifacts": "cp ./out/github/* $CIRCLE_ARTIFACTS",
		"postmerge": "npm install",
		"postinstall": "electron-builder install-app-deps",
		"test": "echo 'Someday there would be tests, I promise'"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"prettier --tab-width 4 --use-tabs --no-semi --single-quote",
			"git add"
		]
	},
	"bugs": "https://github.com/wasd171/chatinder/issues",
	"homepage": "https://github.com/wasd171/chatinder",
	"keywords": [],
	"author": "wasd171",
	"license": "MIT",

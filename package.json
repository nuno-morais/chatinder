{
  "name": "chatinder",
  "productName": "Chatinder",
  "version": "1.6.0",
  "description": "Unofficial desktop messaging client for Tinder.",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wasd171/chatinder.git"
  },
  "scripts": {
    "build:dll": "webpack --config=webpack/dll.js",
    "build:main": "webpack --config=webpack/main.js",
    "build:renderer": "webpack --config=webpack/renderer.js",
    "build:main:dev": "cross-env NODE_ENV=development npm run build:main",
    "build:renderer:dev": "cross-env NODE_ENV=development npm run build:renderer",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production npm run build:dll && npm run build:main && npm run build:renderer",
    "build:dev": "concurrently \"npm run build:main:dev\" \"npm run build:renderer:dev\"",
    "publish": "npm run build:prod && electron-builder",
    "precommit": "lint-staged",
    "circle:clean": "rm -rf node_modules",
    "circle:artifacts": "cp ./out/*.{zip,dmg} $CIRCLE_ARTIFACTS",
    "postmerge": "npm install",
    "postinstall": "electron-builder install-app-deps",
    "test": "echo 'Someday there would be tests, I promise'",
    "build:graphql-to-json": "webpack --config=webpack/graphql-to-json.js",
    "graphql-to-json": "node graphql-to-json",
    "graphql-and-json-to-ts": "apollo-codegen generate src/**/*.graphql --schema schema.json --target typescript --output src/schema.ts",
    "graphql-to-ts": "npm run build:graphql-to-json && npm run graphql-to-json && npm run graphql-and-json-to-ts"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --tab-width 4 --use-tabs --no-semi --single-quote",
      "git add"
    ]
  },
  "bugs": "https://github.com/wasd171/chatinder/issues",
  "homepage": "https://github.com/wasd171/chatinder",
  "keywords": [],
  "author": "wasd171",
  "license": "MIT",
  "dependencies": {
    "about-window": "^1.6.1"
  },
  "devDependencies": {
    "@types/emojione": "^2.2.1",

    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-babili": "^0.0.12",
    "babel-register": "^6.24.0",
    "babili-webpack-plugin": "^0.0.11",
    "concurrently": "^3.4.0",
    "cross-env": "^3.2.4",
